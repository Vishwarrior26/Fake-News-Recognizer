{
  "name": "That's Fake!",
  "description": "Testing",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Click to log",
    "default_popup": "main.html"
  },
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["tf.js","background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "./tf.js",
        "./main.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' blob: filesystem:"
}
